{
  "name": "woqu-react-boilerplate",
  "version": "0.0.1",
  "description": "WoQu's React App Boilerplate",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server",
    "dev:hmr": "NODE_ENV=development HMR=true webpack-dev-server",
    "build": "NODE_ENV=production MINI=true webpack --hide-modules",
    "build:source": "NODE_ENV=production webpack --hide-modules",
    "lint-js": "q-lint-js --verbose; exit 0",
    "precommit": "lint-staged",
    "server:dev": "nodemon ./server/server.js",
    "server:start": "node ./server/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lovefishs/woqu-react-boilerplate.git"
  },
  "keywords": [
    "react",
    "webpack",
    "boilerplate",
    "react-boilerplate",
    "postcss"
  ],
  "author": "changming.huang@woqutech.com",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "q-lint-js": {
    "ignore": [
      "node_modules/",
      "dist/",
      "src/libs/",
      "src/tests/",
      "src/assets/"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "q-lint-js --verbose"
    ]
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "bugs": {
    "url": "https://github.com/lovefishs/woqu-react-boilerplate/issues"
  },
  "homepage": "https://github.com/lovefishs/woqu-react-boilerplate#readme",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "clean-webpack-plugin": "0.1.16",
    "compression-webpack-plugin": "1.0.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "koa": "2.3.0",
    "koa-bodyparser": "4.2.0",
    "koa-qs": "2.0.0",
    "koa-router": "7.2.1",
    "koa-static": "4.0.1",
    "lint-staged": "4.1.0",
    "mockjs": "1.0.1-beta3",
    "nodemon": "1.12.0",
    "postcss-cssnext": "3.0.2",
    "postcss-import": "10.0.0",
    "postcss-loader": "2.0.6",
    "precss": "2.0.0",
    "q-lint-js": "2.3.0",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "3.5.5",
    "webpack-dev-server": "2.7.1"
  },
  "dependencies": {
    "mobx": "3.2.2",
    "mobx-react": "4.2.2",
    "prop-types": "15.5.10",
    "q-theme-default": "0.0.5",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-hot-loader": "3.0.0-beta.7",
    "react-intl": "2.3.0",
    "react-router-dom": "4.2.2"
  }
}
